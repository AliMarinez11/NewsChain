{
    "builds": [
      {
        "src": "api/neutralize.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/narratives.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/neutralize",
        "dest": "api/neutralize.js"
      },
      {
        "src": "/api/narratives",
        "dest": "api/narratives.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }